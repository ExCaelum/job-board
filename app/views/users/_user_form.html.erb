<%= form_for @user do |f| %>
  <%= f.label :username %>
  <%= f.text_field :username %>
  <%= f.label :password %>
  <%= f.password_field :password %>
  <%= f.label :email_address %>
  <%= f.text_field :email_address %>
  <%= f.label :phone_number %>
  <%= f.text_field :phone_number %>
  <%= f.submit "Create Account" %>
<% end %>

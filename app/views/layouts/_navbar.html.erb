<div class='navbar navbar-default navbar-static-top col-md-12'>
    <a href='/' class='navbar-brand'>Captain</a>
    <% if current_user %>
      <ul class="nav navbar-nav">
        <li><%= link_to "#{current_user.username}", user_path(current_user) %></li>
        <% if current_user.admin? %>
          <li><%= link_to "Admin Tags", admin_tags_path %>
          <li><%= link_to "Admin Skills", admin_skills_path %></li>
        <% end %>
      </ul>
      <ul class='nav navbar-nav navbar-right'>
      <li><%= link_to "All Skills", all_skills_path %></li>
      <li><%= link_to "Tags", tags_path %></li>
      <li><%= link_to "Logout", logout_path, method: :delete %></li>
      </ul>
    <% end %>
</div>
